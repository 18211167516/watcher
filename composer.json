{
    "name": "hyperfnew/watcher",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "Hot reload watcher for Hyperf",
    "require": {
        "php": ">=7.4",
        "ext-swoole": ">=4.5",
        "hyperf/command": "~2.2.0",
        "hyperf/di": "~2.2.0",
        "hyperf/framework": "~2.2.0",
        "hyperf/guzzle": "2.2.*"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Watcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Watcher\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hyperf\\Watcher\\ConfigProvider"
        }
    }
}
